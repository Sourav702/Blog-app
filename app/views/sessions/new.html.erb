

<h1>Login</h1>

<%= form_tag sessions_path do |form| %>
  <div class="field">
    <%= label_tag :email %>
    <%= email_field_tag :email,"",required: true %>
  </div>

  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password,"",required: true %>
  </div>

  <div class="actions">
    <%= submit_tag "Login" %>
  </div>
<% end %>
